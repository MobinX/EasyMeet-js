    "source-map-loader": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest",
    "stylelint-config-standard-scss": "latest",
    "stylelint-scss": "latest",
    "ts-jest": "latest",
    "ts-loader": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "webpack": "latest"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17 || ^18",
    "react-dom": "^16.9.0 || ^17 || ^18"
  },
  "dependencies": {
    "process": "^0.11.10",
    "react": "^16.9.0 || ^17 || ^18",
    "react-dom": "^16.9.0 || ^17 || ^18"
  }
}
